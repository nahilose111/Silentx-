FOVCircle.Visible = Aimbot.ShowFOV and Aimbot.Enabled

-- Aim Line
local AimLine = Drawing.new("Line")
AimLine.Visible = false
AimLine.Thickness = 1
AimLine.Transparency = 1
AimLine.Color = Color3.fromRGB(255,255,255)

